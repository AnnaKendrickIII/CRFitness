<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.CRFitness.RoutePlan.model.RoutePlanVO" table="RoutePlan" >
        <id name="route_Plan_Id" column="Route_Plan_Id">
            <generator class="identity"/>
        </id>
        <property name="member_Id" column="Member_Id" />
        <property name="route_Plan_Date" column="Route_Plan_Date" />
        <property name="startPoint" column="RoutePlanStartPoint" />
        <property name="endPoint" column="RoutePlanEndPoint" />
        <property name="mileage" column="Mileage" />
        <property name="routePlanStatus" column="RoutePlanStatus" />
    </class>
</hibernate-mapping>